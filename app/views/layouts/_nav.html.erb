  <div class="container">
    <%= link_to 'Private Events', root_path, class: "navbar-brand" %>
    <div class="collapse navbar-collapse" id="navbarText">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">
        <% if user_signed_in? %>
          <li class="nav-item">
          <%= link_to current_user.name, user_path(current_user), class: "nav-link disabled" %>
          </li>
        <% end %>
      </ul>
      <span class="navbar-nav">
        <% if user_signed_in? %>
          <%= link_to 'My Events', user_path(current_user), class: "nav-item nav-link" %>
          <%= link_to 'Edit Account', edit_user_registration_path, class: "nav-item nav-link" %>
          <%= link_to 'Log Out', destroy_user_session_path, method: :delete, class: "nav-item nav-link" %>
        <% else %>
            <%= link_to 'Log In', user_session_path, class: "nav-item nav-link" %>
            <%= link_to 'Sign Up', new_user_registration_path, class: "nav-item nav-link" %>
        <% end %>
      </span>
    </div>
  </div>


